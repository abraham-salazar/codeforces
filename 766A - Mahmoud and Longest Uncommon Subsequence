#include <bits/stdc++.h>
using namespace std;
 
int main()
{
    string a,b;
    int c, d;
    cin>>a>>b;
    if(a != b){
        c = a.size();
        d = b.size();
        if(c < d){
            cout<<d;
        }
        else{
            cout<<c;
        }
    }else{
        cout<<"-1";
    }
 
    return 0;
}
