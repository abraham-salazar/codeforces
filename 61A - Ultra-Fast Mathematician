#include <bits/stdc++.h>
 
using namespace std;
 
int main()
{
    char a[110], b[110];
    int c[110];
    cin>>a>>b;
    for(int i = 0; i < strlen(a); i++){
        if ((a[i] < b[i]) || (a[i] > b[i]) ){
            c[i] = 1;
        }
        else{
            c[i] = 0;
        }
        cout<<c[i];
 
    }
    return 0;
}
